{
  "name": "ai-chatbot",
  "version": "1.0.0",
  "private": true,
  "description": "AI Chatbot",
  "main": "src/index.js",
  "scripts": {
    "start": "react-scripts start",
    "dev": "DEBUG=* react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "lint": "eslint .",
    "lint:fix": "eslint --fix",
    "format": "prettier --write './**/*.{js,jsx,ts,tsx,css,md,json}' --config ./.prettierrc"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/manifestinteractive/ai-chatbot.git"
  },
  "keywords": [
    "ai",
    "chatbot"
  ],
  "author": "Peter Schmalfeldt",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/manifestinteractive/ai-chatbot/issues"
  },
  "homepage": "https://github.com/manifestinteractive/ai-chatbot#readme",
  "dependencies": {
    "@babel/core": "7.15.5",
    "@heroicons/react": "^2.2.0",
    "@huggingface/inference": "^3.1.5",
    "@react-three/drei": "9.17.3",
    "@react-three/fiber": "8.2.2",
    "babel-plugin-glsl": "1.0.0",
    "babel-plugin-macros": "3.1.0",
    "canvas-sketch-util": "^1.10.0",
    "copy-to-clipboard": "^3.3.3",
    "glsl-curl-noise2": "0.0.4",
    "glsl-noise": "0.0.0",
    "leva": "0.9.29",
    "md5": "^2.3.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-markdown": "^9.0.3",
    "react-scripts": "5.0.1",
    "react-speech-recognition": "^3.10.0",
    "react-toastify": "^11.0.3",
    "rehype-raw": "^7.0.0",
    "three": "0.142.0",
    "v8": "0.1.0"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "react/react-in-jsx-scope": "off"
    },
    "env": {
      "browser": true,
      "es2021": true,
      "jest": true
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.2",
    "prettier": "^3.4.2",
    "sass": "^1.83.4",
    "sass-loader": "^16.0.4",
    "web-vitals": "^4.2.4"
  }
}
